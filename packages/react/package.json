    "./server": {
      "types": "./dist/server/index.d.ts",
      "import": "./dist/server/index.jsx",
      "require": "./dist/server/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "bun",
    "fine-grained",
    "authorization",
    "abac",
    "rbac"
